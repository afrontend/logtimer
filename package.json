{
  "name": "@afrontend/logtimer",
  "version": "0.0.3",
  "description": "check the interval time in the log",
  "homepage": "https://github.com/afronend/logtimer",
  "author": {
    "name": "Bob Hwang",
    "email": "frontend.wordpress@gmail.com",
    "url": "https://agvim.wordpress.com/"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "bin": {
    "logtimer": "lib/logtimer.js"
  },
  "keywords": [
    "log",
    "interval time"
  ],
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint": "^5.9.0",
    "prettier": "^1.15.2",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-config-xo": "^0.25.0",
    "jest": "^23.5.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest",
    "test:cmd": "cat lib/__tests__/test.log | node lib/logtimer.js"
  },
  "repository": "https://github.com/afrontend/logtimer.git",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT",
  "dependencies": {
    "cli": "^1.0.1",
    "cosmiconfig": "^5.2.0",
    "moment": "^2.24.0",
    "readline": "^1.3.0"
  }
}
